@book{baker_taille_2004,
	address = {Paris},
	title = {Taille des pensées},
	isbn = {978-2-267-01705-2},
	abstract = {Avez-vous jamais essayé de mesurer la taille de vos pensées? Si oui, vous savez sûrement déjà que " la plupart ont trois pieds de haut et le degré de complexité d'un moteur de tondeuse ", comme nous le révèle ici même Nicholson Baker. Si non, plongez avec délectation dans l'univers du romancier américain, qui s'avère pour l'occasion essayiste hors pair. Dans la meilleure tradition des polygraphes érudits et flâneurs de ce XVIIIe siècle qu'à l'évidence il admire, Baker nous offre avec La Taille des pensées un savoureux mélange de réflexions philosophiques, de conseils et de morales élémentaires, pimenté d'anecdotes intimes. De sa plume trempée dans un puissant philtre d'humour, il croque avec une précision diabolique les menus objets du quotidien et les fantasmes qu'ils suscitent, les souvenirs d'enfance ou les micro-événements de la vie. Et, surtout, il dit les livres et la fascination qu'ils exercent sur lui lus, cités, critiqués, manipulés, caressés, soupesés puis amoureusement replacés sur les rayons d'une bibliothèque digne de celle qu'inventa Borges, les livres sont les véritables héros de ces essais qui en assurent la brillante défense et illustration},
	language = {Français},
	publisher = {Christian Bourgois Editeur},
	author = {Baker, Nicholson and Cazé, Antoine},
	month = jan,
	year = {2004}
}

@book{barthes_aventure_2015,
	title = {L'aventure sémiologique},
	isbn = {978-2-7578-5430-3},
	abstract = {Il y eut, dans l'aventure sémiologique, un bonheur de la science ; il y eut aussi l'anxiété de la dépasser, de ne pas laisser se refermer le sens. C'est ce double mouvement qu'on verra à l'œuvre dans les travaux réunis ici, où Roland Barthes pose les assises de cette discipline nouvelle, notamment Éléments de sémiologie, L'Ancienne Rhétorique, Introduction à l'analyse structurale des récits. On trouvera, à côté d'eux, une série de projets sur ce que pourrait être l'enquête sémiologique méthodique dans des domaines aussi divers que l'objet, la publicité, l'ethnologie, l'urbanisme, la médecine. Enfin, la mise en œuvre de l'analyse structurale sur deux fragments de la Bible et sur un conte de Poe.Roland Barthes (1915-1980) Il a élaboré une pensée critique singulière, en constant dialogue avec la pluralité des discours théoriques et des mouvements intellectuels de son époque, tout en dénonçant le pouvoir de tout langage institué. Il est notamment l'auteur du Degré zéro de l'écriture (1953) et de Fragments d'un discours amoureux (1977).},
	language = {Français},
	publisher = {Points},
	author = {Barthes, Roland},
	month = nov,
	year = {2015}
}

